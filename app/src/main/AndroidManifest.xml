<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ours.thecanteen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="OUR CANTEEN"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SnacksMenuActivity" />
        <activity android:name=".BreakfastMenuActivity" />
        <activity android:name=".CostBarActivity" />
        <activity android:name=".OptionActivity" />
        <activity android:name=".LunchMenuActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".DrinksMenuActivity" />
        <activity android:name=".CartMenuActivity" />
        <activity android:name=".CreditsMenuActivity" />
        <activity android:name=".OrderDetailsActivity"/>
    </application>

</manifest>